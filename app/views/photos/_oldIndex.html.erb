<table>
  <thead>
    <tr>
      <!-- <th>Image data</th> -->
      <th>Photo</th>
      <th>User</th>
      <th>Caption</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <!-- <td><%= #photo.image_data %></td> -->
        <td><img src="<%=photo.image_url%>"></td>
        <td><%= photo.user.email %></td>
        <td><%= photo.caption %></td>
        <!-- <td><%=# link_to 'Show', photo%></td> -->
        <td><%= link_to 'Show', photo, class: "index_button" %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo), class: "index_button" %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, class: "index_button", data: { confirm: 'Are you sure?' } %></td>
        <td>
          <%= link_to 'Comment?', new_comment_path%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
